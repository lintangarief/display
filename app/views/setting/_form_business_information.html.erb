<form class="form-horizontal" role="form">
  <div class="form-group">
    <label class="col-lg-3 control-label">Business name:</label>
    <div class="col-lg-8">
      <input class="form-control" type="text" value="Business Name">
    </div>
  </div>
  <div class="form-group">
    <label class="col-lg-3 control-label">Address:</label>
    <div class="col-lg-8">
      <input class="form-control" type="text" value="Address">
    </div>
  </div>
  <div class="form-group">
    <label class="col-lg-3 control-label">City:</label>
    <div class="col-lg-8">
      <input class="form-control" type="text" value="City">
    </div>
  </div>
  <div class="form-group">
    <label class="col-md-3 control-label">State:</label>
    <div class="col-md-8">
      <input class="form-control" type="text" value="state">
    </div>
  </div>
  <div class="form-group">
    <label class="col-md-3 control-label">Zip:</label>
    <div class="col-md-8">
      <input class="form-control" type="text" value="zipcode">
    </div>
  </div>
  <div class="form-group">
    <label class="col-lg-3 control-label">Business email:</label>
    <div class="col-lg-8">
      <input class="form-control" type="email" value="Business Email">
    </div>
  </div>
  <div class="form-group">
    <label class="col-md-3 control-label"></label>
    <div class="col-md-8">
      <input type="button" class="btn btn-primary" value="Save Changes">
      <span></span>
      <input type="reset" class="btn btn-default" value="Cancel">
    </div>
  </div>
</form>
